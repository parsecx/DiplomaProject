    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <RootNamespace>AdmiPanel_HealthApp</RootNamespace>
    <UserSecretsId>87fc62e2-114a-402a-9d8d-391f519ca551</UserSecretsId>
    <DockerDefaultTargetOS>Linux</DockerDefaultTargetOS>
